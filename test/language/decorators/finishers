var finishCalled = [];

function finish(element) {
  element.finisher = function runFinisher(target) {
    assert.equal(X, target);
    finishCalled.push(element.key);
  };
  return element;
}

class X {
  @finish x() {}
  @finish static a() {}
  @finish y() {}
}

assert.deepEqual(['x', 'a', 'y'], finishCalled);
